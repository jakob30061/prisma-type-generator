{
  "name": "@jakob30061/prisma-type-generator",
  "version": "1.0.0",
  "description": "Automatically generate Typescript enums from you prisma schema",
  "main": "dist/index.js",
  "repository": "https://github.com/jakob30061/prisma-type-generator.git",
  "author": "jakob30061",
  "license": "GPL-3.0-only",
  "keywords": [
    "prisma",
    "typescript",
    "prisma-types",
    "types",
    "interface",
    "enums"
  ],
  "bugs": {
    "url": "https://github.com/jakob30061/prisma-type-generator/issues"
  },
  "files": [
    "dist/",
    "COPYING",
    "README.md",
    "package.json"
  ],

  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "build": "pnpm clean && vite build",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "prisma-type-generator": "dist/index.js"
  },
  "devDependencies": {
    "@types/node": "16.4.3",
    "@types/prettier": "2.3.2",
    "semantic-release": "24.2.5",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@prisma/generator-helper": "^6.8.2",
    "prettier": "^3.5.3"
  }
}
